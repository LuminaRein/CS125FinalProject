<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <RelativeLayout
            android:id="@+id/relativeLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="match_parent"
                android:layout_height="480dp"
                android:layout_below="@+id/imageView2"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="1dp"
                android:contentDescription="@android:string/untitled"
                app:srcCompat="@android:color/holo_orange_dark"
                tools:srcCompat="@android:color/holo_orange_dark" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="match_parent"
                android:layout_height="213dp"
                android:contentDescription="@android:string/untitled"
                app:srcCompat="@android:color/holo_orange_light"
                tools:srcCompat="@android:color/holo_orange_light" />

            <ImageButton
                android:id="@+id/button0"
                android:layout_width="72dp"
                android:layout_height="71dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="51dp"
                android:layout_marginLeft="51dp"
                android:layout_marginEnd="288dp"
                android:layout_marginRight="288dp"
                android:layout_marginBottom="298dp"
                app:srcCompat="@drawable/circle" />

            <ImageButton
                android:id="@+id/button5"
                android:layout_width="72dp"
                android:layout_height="71dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="65dp"
                android:layout_marginRight="65dp"
                android:layout_marginBottom="177dp"
                app:srcCompat="@drawable/circle" />

            <ImageButton
                android:id="@+id/button3"
                android:layout_width="72dp"
                android:layout_height="71dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="50dp"
                android:layout_marginLeft="50dp"
                android:layout_marginEnd="289dp"
                android:layout_marginRight="289dp"
                android:layout_marginBottom="178dp"
                app:srcCompat="@drawable/triangle_middle"
                tools:srcCompat="@drawable/triangle_middle" />

            <ImageButton
                android:id="@+id/button7"
                android:layout_width="72dp"
                android:layout_height="71dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="181dp"
                android:layout_marginRight="179dp"
                android:layout_marginBottom="64dp"
                app:srcCompat="@drawable/triangle_middle"
                tools:srcCompat="@drawable/triangle_middle" />

            <ImageButton
                android:id="@+id/button4"
                android:layout_width="72dp"
                android:layout_height="71dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="181dp"
                android:layout_marginRight="179dp"
                android:layout_marginBottom="175dp"
                app:srcCompat="@drawable/triangle_right_down"
                tools:srcCompat="@drawable/triangle_right_down" />

            <ImageButton
                android:id="@+id/button8"
                android:layout_width="72dp"
                android:layout_height="71dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="65dp"
                android:layout_marginRight="65dp"
                android:layout_marginBottom="63dp"
                app:srcCompat="@drawable/triangle_right_down"
                tools:srcCompat="@drawable/triangle_right_down" />

            <ImageButton
                android:id="@+id/button1"
                android:layout_width="72dp"
                android:layout_height="71dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="137dp"
                android:layout_marginEnd="181dp"
                android:layout_marginRight="179dp"
                android:layout_marginBottom="300dp"
                app:srcCompat="@drawable/triangle_right_left"
                tools:srcCompat="@drawable/triangle_right_left" />

            <ImageButton
                android:id="@+id/button6"
                android:layout_width="72dp"
                android:layout_height="71dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="51dp"
                android:layout_marginLeft="51dp"
                android:layout_marginEnd="288dp"
                android:layout_marginRight="288dp"
                android:layout_marginBottom="62dp"
                app:srcCompat="@drawable/triangle_right_left"
                tools:srcCompat="@drawable/triangle_right_left" />

            <ImageButton
                android:id="@+id/button2"
                android:layout_width="72dp"
                android:layout_height="71dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="66dp"
                android:layout_marginRight="66dp"
                android:layout_marginBottom="302dp"
                app:srcCompat="@drawable/diamond"
                tools:srcCompat="@drawable/diamond" />

            <Button
                android:id="@+id/return_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/imageView2"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginEnd="51dp"
                android:layout_marginRight="51dp"
                android:layout_marginBottom="44dp"
                android:text="Return" />

            <Button
                android:id="@+id/rule_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/imageView2"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginEnd="174dp"
                android:layout_marginRight="174dp"
                android:layout_marginBottom="35dp"
                android:text="RULES" />

            <Button
                android:id="@+id/restart_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/imageView2"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginEnd="294dp"
                android:layout_marginRight="294dp"
                android:layout_marginBottom="47dp"
                android:text="Restart" />

            <EditText
                android:id="@+id/scoreLabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/imageView2"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginEnd="69dp"
                android:layout_marginRight="69dp"
                android:layout_marginBottom="110dp"
                android:clickable="false"
                android:duplicateParentState="false"
                android:editable="false"
                android:ems="10"
                android:inputType="none"
                android:text="Score: 1314"
                android:textSize="24sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/highScoreTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/imageView2"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginEnd="37dp"
                android:layout_marginRight="37dp"
                android:layout_marginBottom="160dp"
                android:text="High Score: 10000"
                android:textStyle="italic" />

        </RelativeLayout>
    </LinearLayout>
</layout>
